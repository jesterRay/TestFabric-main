glish\"}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"option\",{children:\"France\"}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"option\",{children:\"Bangla\"}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"option\",{children:\"Hindi\"})]})})})})]})})});}/* harmony default export */ var Header1_Topbar3 = (Topbar3);\n// CONCATENATED MODULE: ./src/components/Header1/index.jsx\nfunction Header1(){return/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(jsx_runtime[\"Fragment\"],{children:[/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(Header1_Topbar3,{}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(Header1_MainMenu3,{})]});}/* harmony default export */ var components_Header1 = (Header1);\n// CONCATENATED MODULE: ./src/components/PageBanner/index.jsx\nfunction PageBanner(_ref){let{bannerBg,currentPage,heading,breadCrumb=true}=_ref;return/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"section\",{className:\"page-banner-wrap-2 \"// style={{ backgroundImage: `url(${bannerBg})` }}\n,children:/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"div\",{className:\"container\",children:/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"div\",{className:\"row\",children:/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"div\",{className:\"col-12 col-lg-12\",children:[breadCrumb&&/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"div\",{className:\"breadcrumb-wrap\",children:/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"nav\",{children:/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"ol\",{className:\"breadcrumb\",children:[/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"li\",{className:\"breadcrumb-item\",children:/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"a\",{href:\"index.html\",children:\"Home\"})}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"li\",{className:\"breadcrumb-item active\",\"aria-current\":\"page\",children:currentPage})]})})}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"div\",{className:\"page-heading\",children:/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"h2\",{children:heading})})]})})})});}/* harmony default export */